@model string
<div style="display: flex;">
    @Html.EJS().Menu((Model + "_menu")).Items(ViewBag.menuItems).Select("selectHandler").Render()
    @if (ViewBag.subMenuItems[0].Items.Count > 0)
    {
        @Html.EJS().Menu(Model + "_menu2").Items(ViewBag.subMenuItems).ShowItemOnClick(true).Select("subMenuSelectHandler").BeforeOpen("beforeOpen").OnClose("onClose").Render()
    }
</div>
@Html.EJS().ScriptManager()